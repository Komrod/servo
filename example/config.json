{
  "server": {
    "port": "80",
    "dir": "www/"
  },
  "page": {
    "default": "index.html",
    "script": "xjs",
    "error": {
      "401": "page/401.html",
      "404": "page/404.html",
      "500": "page/500.html"
    }
  },
  "log": {
    "access": {
      "enabled": true,
      "path": "log/access.log",
      "console": false
    },
    "error": {
      "debug": true,
      "enabled": true,
      "path": "log/error.log",
      "console": true
    },
    "warning": {
      "enabled": true
    }
  }
}